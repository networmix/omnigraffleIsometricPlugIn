{
  "author": "Andrey Golovanov",
  "identifier": "com.networmix.OmniGraffleIsometricPlugin",
  "version": "1.2",
  "description": "Transform 2D shapes and lines into isometric projections with one click.",
  "defaultLocale": "en",
  "libraries": [
    {
      "identifier": "IsometricLib"
    }
  ],
  "actions": [
    {
      "identifier": "makeLeftPlane",
      "image": "square.lefthalf.filled"
    },
    {
      "identifier": "makeRightPlane",
      "image": "square.righthalf.filled"
    },
    {
      "identifier": "makeTopLeftPlane",
      "image": "square.tophalf.filled"
    },
    {
      "identifier": "makeTopRightPlane",
      "image": "square.bottomhalf.filled"
    }
  ]
}
